# .gitignore for a full-stack project with a frontend and backend
/frontend/.env
/backend/.env   

# Node modules
/backend/node_modules/
/frontend/node_modules/

# Logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# OS files
.DS_Store
Thumbs.db

# Environment variables
.env
.env.*  # In case you use multiple (e.g., .env.production)

# Build output
/frontend/dist/
/frontend/build/
/backend/dist/
/backend/build/

# IDE configs
.vscode/
.idea/

# Dependency lock files (optional: keep if you want consistency across installs)
/frontend/package-lock.json
/backend/package-lock.json

/archive
